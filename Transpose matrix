class Solution {
    public int[][] transpose(int[][] matrix) {
        int row_size = matrix.length;
        int col_size = matrix[0].length;
        int[][] transpose = new int[col_size][row_size];
        for(int i=0;i<row_size;i++){
            for(int j=0;j<col_size;j++){
                transpose[j][i] = matrix[i][j];
            }
        }
        return transpose;
    }
}
